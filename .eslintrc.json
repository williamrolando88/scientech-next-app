{
  "root": true,
  "extends": [
    "next",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      2,
      {
        "argsIgnorePattern": "^_",
        "args": "after-used",
        "varsIgnorePattern": "^_"
      }
    ],
    "no-console": [2, { "allow": ["warn", "error"] }]
  }
}
